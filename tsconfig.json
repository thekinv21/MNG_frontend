{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true
	},
	"paths": {
		"@/*": ["./src/*"],
		"@/api/*": ["./src/api/*"],
		"@/assets/*": ["./src/assets/*"],
		"@/constants/*": ["./src/constants/*"],
		"@/components/*": ["./src/components/*"],
		"@/layout/*": ["./src/components/layout/*"],
		"@/config/*": ["./src/config/*"],
		"@/hooks/*": ["./src/hooks/*"],
		"@/pages/*": ["./src/pages/*"],
		"@/providers/*": ["./src/providers/*"],
		"@/ui/*": ["./src/components/ui/*"],
		"@/router/*": ["./src/router/*"],
		"@/services/*": ["./src/services/*"],
		"@/store/*": ["./src/store/*"],
		"@/types/*": ["./src/types/*"],
		"@/utils/*": ["./src/utils/*"]
	}
}
